{
  "name": "ngx-sentry",
  "description": "Angular ErrorHandler and HttpInterceptor for Sentry.",
  "version": "1.4.0",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/ngx-sentry.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/ngx-sentry/issues"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-sentry-demo",
    "build": "ng build --prod",
    "postbuild": "cp README.md dist/ngx-sentry/README.md && cp LICENSE.md dist/ngx-sentry/LICENSE.md && cp CHANGELOG.md dist/ngx-sentry/CHANGELOG.md",
    "lint": "ng lint",
    "check": "[[ $TRAVIS_PULL_REQUEST == false ]] && snyk test && snyk monitor",
    "format": "prettier --write \"{src,projects}/**/*.ts\"",
    "changelog": "standard-changelog",
    "link": "yarn run build && cd dist/ngx-sentry && yarn link",
    "unlink": "cd dist/ngx-sentry && yarn unlink"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  },
  "dependencies": {
    "@angular/animations": "~7.2.6",
    "@angular/common": "~7.2.6",
    "@angular/compiler": "~7.2.6",
    "@angular/core": "~7.2.6",
    "@angular/forms": "~7.2.6",
    "@angular/platform-browser": "~7.2.6",
    "@angular/platform-browser-dynamic": "~7.2.6",
    "@angular/router": "~7.2.6",
    "core-js": "^2.6.5",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.3",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.3",
    "@angular-devkit/build-ng-packagr": "~0.13.5",
    "@angular/cli": "~7.3.3",
    "@angular/compiler-cli": "~7.2.6",
    "@angular/language-service": "~7.2.6",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@sentry/browser": "^4.6.4",
    "@types/node": "~11.11.0",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.4",
    "ng-packagr": "^4.7.1",
    "prettier": "^1.16.4",
    "snyk": "^1.136.1",
    "standard-changelog": "^2.0.7",
    "ts-node": "~8.0.2",
    "tsickle": "^0.34.3",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.2"
  }
}
