{
  "name": "ngx-sentry",
  "description": "Angular ErrorHandler and HttpInterceptor for Sentry.",
  "version": "1.3.1",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/ngx-sentry.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/ngx-sentry/issues"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-sentry-demo",
    "build": "ng build",
    "postbuild": "cp README.md dist/ngx-sentry/README.md && cp LICENSE.md dist/ngx-sentry/LICENSE.md",
    "lint": "ng lint",
    "check": "snyk test && snyk monitor",
    "format": "prettier --write \"{src,projects}/**/*.ts\"",
    "changelog": "standard-changelog --release-count 1",
    "link": "yarn run build && cd dist/ngx-sentry && yarn link",
    "unlink": "cd dist/ngx-sentry && yarn unlink"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint --env HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "commitlint": {
    "extends": ["@commitlint/config-conventional"],
    "rules": {
      "header-max-length": [2, "always", 100]
    }
  },
  "lint-staged": {
    "{src,projects}/**/*.ts": [
      "prettier --write",
      "git add"
    ]
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@sentry/browser": "^4.5.2",
    "core-js": "^2.6.2",
    "rxjs": "~6.3.3",
    "standard-changelog": "^2.0.6",
    "tslib": "^1.9.3",
    "zone.js": "~0.8.27"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular-devkit/build-ng-packagr": "~0.13.0",
    "@angular/cli": "~7.3.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@commitlint/cli": "^7.3.1",
    "@commitlint/config-conventional": "^7.3.1",
    "@types/node": "~10.12.18",
    "codelyzer": "~4.5.0",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.0",
    "ng-packagr": "^4.7.0",
    "prettier": "^1.15.3",
    "snyk": "^1.122.0",
    "ts-node": "~8.0.2",
    "tsickle": ">=0.34.0",
    "tslib": "^1.9.3",
    "tslint": "^5.12.1",
    "tslint-config-prettier": "^1.17.0",
    "typescript": "~3.2.2"
  }
}
