{
  "name": "ngx-sentry",
  "description": "Angular ErrorHandler and HttpInterceptor for Sentry.",
  "version": "2.0.0",
  "license": "MIT",
  "author": {
    "name": "Pascal Iske",
    "email": "info@pascal-iske.de",
    "url": "https://pascal-iske.de"
  },
  "repository": {
    "url": "https://github.com/pascaliske/ngx-sentry.git",
    "type": "git"
  },
  "bugs": {
    "url": "https://github.com/pascaliske/ngx-sentry/issues"
  },
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "ng serve ngx-sentry-demo",
    "build": "ng build --prod",
    "postbuild": "cp README.md dist/ngx-sentry/README.md && cp LICENSE.md dist/ngx-sentry/LICENSE.md && cp CHANGELOG.md dist/ngx-sentry/CHANGELOG.md",
    "lint": "ng lint",
    "check": "snyk test && snyk monitor",
    "format": "prettier --write \"{src,projects}/**/*.ts\"",
    "changelog": "standard-changelog",
    "link": "yarn run build && cd dist/ngx-sentry && yarn link",
    "unlink": "cd dist/ngx-sentry && yarn unlink"
  },
  "greenkeeper": {
    "ignore": [
      "typescript"
    ]
  },
  "dependencies": {
    "@angular/animations": "~7.2.12",
    "@angular/common": "~7.2.12",
    "@angular/compiler": "~7.2.12",
    "@angular/core": "~7.2.12",
    "@angular/forms": "~7.2.12",
    "@angular/platform-browser": "~7.2.12",
    "@angular/platform-browser-dynamic": "~7.2.12",
    "@angular/router": "~7.2.12",
    "core-js": "^3.1.0",
    "rxjs": "~6.4.0",
    "tslib": "^1.9.3",
    "zone.js": "~0.8.29"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.7",
    "@angular-devkit/build-ng-packagr": "~0.13.7",
    "@angular/cli": "~7.3.7",
    "@angular/compiler-cli": "~7.2.12",
    "@angular/language-service": "~7.2.12",
    "@commitlint/cli": "^7.5.2",
    "@commitlint/config-conventional": "^7.5.0",
    "@sentry/browser": "^5.0.5",
    "@types/node": "~12.0.0",
    "codelyzer": "~5.0.0",
    "husky": "^2.0.0",
    "lint-staged": "^8.1.5",
    "ng-packagr": "^5.0.1",
    "prettier": "^1.16.4",
    "snyk": "^1.147.2",
    "standard-changelog": "^2.0.7",
    "ts-node": "~8.1.0",
    "tsickle": "^0.35.0",
    "tslib": "^1.9.3",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "~3.2.2"
  }
}
